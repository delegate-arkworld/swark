<?xml version="1.0" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <imports>
        <!-- Parameters -->
        <import resource="services/parameter.xml"/>

        <!-- Services -->
        <import resource="services/service.xml"/>

        <!-- Helper -->
        <import resource="services/helper.xml"/>

        <!-- Subscriber -->
        <import resource="services/subscriber.xml"/>

        <!-- Commands -->
        <import resource="services/command.xml"/>

        <!-- ArkClient -->
        <import resource="../Components/ArkClient/Resources/services.xml"/>
    </imports>
</container>
